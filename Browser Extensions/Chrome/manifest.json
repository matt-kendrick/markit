{
    "manifest_version": 2,
    "name": "MarkIt",
    "description": "Sends current url to MarkIt: Online Bookmark Manager.",
    "version": "1.0.0",
    "icons": { 
        "16": "icon16.png",
        "48": "icon48.png",
       "128": "icon128.png" },
    "browser_action": {
        "default_icon": "icon48.png",
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab","storage","http://pastepad.mattkendrick.com/*"],
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}